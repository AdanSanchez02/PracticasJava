import java.util.Random;

public class Test {

	private static char randomChar() {
		int rand = new Random().nextInt(52);
		char start = (rand < 26) ? 'A' : 'a';
		return (char) (start + rand % 26);
	}

	public static void main(String[] args) {
		char c = randomChar();
		System.out.println(c);
	}

}
